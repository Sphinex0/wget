          </mrow>
        </msub>
        <mo>=</mo>
        <mi>φ<!-- φ --></mi>
        <mo stretchy="false">(</mo>
        <msub>
          <mi>x</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msub>
        <mo>−<!-- − --></mo>
        <msub>
          <mi>x</mi>
