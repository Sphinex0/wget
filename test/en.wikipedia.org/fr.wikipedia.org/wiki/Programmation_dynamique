        <mo stretchy="false">)</mo>
        <mo stretchy="false">)</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle c(x)=v(x)+\max(c(g(x)),c(d(x)))}</annotation>
  </semantics>
