                  <mo>,</mo>
                  <mi>j</mi>
                  <mo stretchy="false">)</mo>
                  <mo>,</mo>
                  <mi>q</mi>
                  <mo stretchy="false">(</mo>
                  <mi>i</mi>
                  <mo>−<!-- − --></mo>
