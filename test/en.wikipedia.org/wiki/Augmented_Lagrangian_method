        <mo>−<!-- − --></mo>
        <mi>c</mi>
        <msup>
          <mo fence="false" stretchy="false">‖<!-- ‖ --></mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>2</mn>
          </mrow>
        </msup>
        <mo>+</mo>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mrow>
              <mo fence="false" stretchy="false">‖<!-- ‖ --></mo>
              <mi>x</mi>
              <mo>−<!-- − --></mo>
              <msub>
                <mi>x</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mi>k</mi>
                </mrow>
              </msub>
