                        <mn>2</mn>
                      </mrow>
                    </msup>
                    <mn>2</mn>
                  </mfrac>
                </mrow>
                <mo>+</mo>
                <mi>O</mi>
                <mo stretchy="false">(</mo>
                <msup>
