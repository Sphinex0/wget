            <msub>
              <mi>x</mi>
              <mrow class="MJX-TeXAtom-ORD">
                <mn>0</mn>
              </mrow>
            </msub>
          </mrow>
        </munder>
        <mrow class="MJX-TeXAtom-ORD">
          <mfrac>
            <mrow>
