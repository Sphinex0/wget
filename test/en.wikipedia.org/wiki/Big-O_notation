        <mo>,</mo>
        <mo>∼<!-- ∼ --></mo>
        <mo>,</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle o,O,\Theta ,\sim ,}</annotation>
