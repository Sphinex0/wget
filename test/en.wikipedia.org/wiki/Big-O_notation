        <mo stretchy="false">)</mo>
        <mo>=</mo>
        <mi>o</mi>
        <mrow class="MJX-TeXAtom-ORD">
          <mrow class="MJX-TeXAtom-OPEN">
            <mo maxsize="1.2em" minsize="1.2em">(</mo>
          </mrow>
        </mrow>
