            </mrow>
            <mrow>
              <mi mathvariant="normal">∂<!-- ∂ --></mi>
              <mi>t</mi>
            </mrow>
          </mfrac>
        </mrow>
        <mo>=</mo>
        <munder>
          <mo movablelimits="true" form="prefix">max</mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>u</mi>
          </mrow>
        </munder>
        <mrow>
          <mo>{</mo>
          <mrow>
            <mi>I</mi>
            <mo stretchy="false">(</mo>
