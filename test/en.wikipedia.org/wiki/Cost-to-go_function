        <mo>≤<!-- ≤ --></mo>
        <mi>t</mi>
        <mo>≤<!-- ≤ --></mo>
        <msub>
          <mi>t</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mn>1</mn>
          </mrow>
