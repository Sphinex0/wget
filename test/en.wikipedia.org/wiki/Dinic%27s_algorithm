        <mo>,</mo>
        <mn>1</mn>
        <mo>,</mo>
        <mn>3</mn>
        <mo>,</mo>
        <mi>t</mi>
        <mo fence="false" stretchy="false">}</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle \{s,1,3,t\}}</annotation>
  </semantics>
