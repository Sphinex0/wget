        <mo>,</mo>
        <mi>n</mi>
        <mo stretchy="false">)</mo>
        <mo>≤<!-- ≤ --></mo>
        <mi>f</mi>
        <mo stretchy="false">(</mo>
        <mi>t</mi>
        <mo>+</mo>
        <mn>1</mn>
