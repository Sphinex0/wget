                        <mi>k</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mi>a</mi>
                        </mrow>
                      </msup>
                    </mrow>
                    <mrow>
                      <mn>1</mn>
                      <mo>+</mo>
                      <mi>a</mi>
                      <mi>b</mi>
                      <mo>+</mo>
                      <msup>
                        <mi>a</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mn>2</mn>
                        </mrow>
                      </msup>
                      <msup>
                        <mi>b</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mn>2</mn>
                        </mrow>
                      </msup>
                      <mo>+</mo>
                      <mo>…<!-- … --></mo>
                      <mo>+</mo>
                      <msup>
                        <mi>a</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mi>T</mi>
                          <mo>−<!-- − --></mo>
                          <mn>2</mn>
                        </mrow>
                      </msup>
                      <msup>
                        <mi>b</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mi>T</mi>
                          <mo>−<!-- − --></mo>
                          <mn>2</mn>
                        </mrow>
                      </msup>
                    </mrow>
                  </mfrac>
                </mrow>
              </mtd>
            </mtr>
