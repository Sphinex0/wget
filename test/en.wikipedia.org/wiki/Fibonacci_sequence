                <mrow class="MJX-TeXAtom-ORD">
                  <mfrac>
                    <mrow>
                      <msub>
                        <mi>U</mi>
                        <mrow class="MJX-TeXAtom-ORD">
                          <mn>0</mn>
                        </mrow>
                      </msub>
                      <mi>φ<!-- φ --></mi>
                      <mo>−<!-- − --></mo>
