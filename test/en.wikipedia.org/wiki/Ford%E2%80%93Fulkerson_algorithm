        <mo stretchy="false">←<!-- ← --></mo>
        <mi>f</mi>
        <mo stretchy="false">(</mo>
        <mi>v</mi>
        <mo>,</mo>
        <mi>u</mi>
        <mo stretchy="false">)</mo>
        <mo>−<!-- − --></mo>
        <msub>
          <mi>c</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>f</mi>
          </mrow>
        </msub>
