          <mfrac>
            <mrow>
              <mi mathvariant="normal">∂<!-- ∂ --></mi>
              <mi>sin</mi>
              <mo>⁡<!-- ⁡ --></mo>
              <mi>x</mi>
            </mrow>
            <mrow>
              <mi mathvariant="normal">∂<!-- ∂ --></mi>
              <mi>x</mi>
            </mrow>
          </mfrac>
        </mrow>
        <mo stretchy="false">(</mo>
        <mn>0</mn>
        <mo stretchy="false">)</mo>
        <mo>=</mo>
        <mn>1</mn>
        <mo>,</mo>
