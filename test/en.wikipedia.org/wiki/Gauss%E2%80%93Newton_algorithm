        </mrow>
        <mo stretchy="false">)</mo>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle \mathbf {f} (\mathbf {x} ,{\boldsymbol {\beta }})}</annotation>
  </semantics>
