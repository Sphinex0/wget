        <mi>t</mi>
        <mspace width="thickmathspace"></mspace>
        <mi>e</mi>
        <mi>e</mi>
        <mi>e</mi>
        <mspace width="thickmathspace"></mspace>
        <mi>u</mi>
      </mstyle>
