                  </mrow>
                </msup>
                <mspace width="negativethinmathspace"></mspace>
                <mo>,</mo>
              </mtd>
            </mtr>
            <mtr>
              <mtd>
                <mi>β<!-- β --></mi>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <mn>2</mn>
                <mi>arctan</mi>
