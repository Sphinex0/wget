                    <mi>i</mi>
                  </mrow>
                </msup>
                <mo>+</mo>
                <msup>
                  <mi>e</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mo>−<!-- − --></mo>
                    <mi>π<!-- π --></mi>
                    <mi>i</mi>
                  </mrow>
                </msup>
              </mtd>
