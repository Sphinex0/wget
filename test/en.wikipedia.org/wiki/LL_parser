            </mrow>
          </mover>
        </mrow>
        <mtext>&nbsp;</mtext>
        <mi>E</mi>
        <mtext>&nbsp;</mtext>
        <mrow class="MJX-TeXAtom-ORD">
          <mover>
            <mo stretchy="false">⇒<!-- ⇒ --></mo>
            <mrow>
              <mo stretchy="false">(</mo>
              <mn>2</mn>
              <mo stretchy="false">)</mo>
            </mrow>
          </mover>
        </mrow>
        <mtext>&nbsp;</mtext>
        <mo stretchy="false">(</mo>
        <mi>E</mi>
        <mo>+</mo>
        <mi>E</mi>
        <mo stretchy="false">)</mo>
        <mtext>&nbsp;</mtext>
        <mrow class="MJX-TeXAtom-ORD">
          <mover>
