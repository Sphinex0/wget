          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
          <mrow class="MJX-TeXAtom-ORD">
            <mi mathvariant="normal">⊤<!-- ⊤ --></mi>
          </mrow>
        </msubsup>
        <mo stretchy="false">)</mo>
        <msub>
          <mi>H</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
        </msub>
        <mo stretchy="false">(</mo>
        <mi>I</mi>
        <mo>−<!-- − --></mo>
        <msub>
          <mi>ρ<!-- ρ --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
        </msub>
