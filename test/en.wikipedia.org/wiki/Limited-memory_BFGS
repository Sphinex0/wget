        <mo stretchy="false">)</mo>
        <mo>+</mo>
        <msub>
          <mi>ρ<!-- ρ --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
        </msub>
        <msub>
          <mi>s</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>k</mi>
          </mrow>
        </msub>
        <msubsup>
          <mi>s</mi>
          <mrow class="MJX-TeXAtom-ORD">
