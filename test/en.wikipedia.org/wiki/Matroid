        <mo>⊆<!-- ⊆ --></mo>
        <mi>R</mi>
        <mo>∪<!-- ∪ --></mo>
        <mi>G</mi>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle g\in X\subseteq R\cup G}</annotation>
  </semantics>
</math></span><img src="https://wikimedia.org/api/rest_v1/media/math/render/svg/c0c9a3c193bcd047a40e7d3fc9f9e4deba4359a9" class="mwe-math-fallback-image-inline mw-invert skin-invert" aria-hidden="true" style="vertical-align: -0.671ex; width:15.208ex; height:2.509ex;" alt="{\displaystyle g\in X\subseteq R\cup G}"></span> or a <span class="mwe-math-element mwe-math-element-inline"><span class="mwe-math-mathml-inline mwe-math-mathml-a11y" style="display: none;"><math xmlns="http://www.w3.org/1998/Math/MathML" alttext="{\displaystyle Y\in D}">
  <semantics>
    <mrow class="MJX-TeXAtom-ORD">
      <mstyle displaystyle="true" scriptlevel="0">
        <mi>Y</mi>
        <mo>∈<!-- ∈ --></mo>
        <mi>D</mi>
      </mstyle>
    </mrow>
    <annotation encoding="application/x-tex">{\displaystyle Y\in D}</annotation>
  </semantics>
</math></span><img src="https://wikimedia.org/api/rest_v1/media/math/render/svg/93a0279bb586bc7e4af0209a9736d888bd2bdb60" class="mwe-math-fallback-image-inline mw-invert skin-invert" aria-hidden="true" style="vertical-align: -0.338ex; width:6.538ex; height:2.176ex;" alt="{\displaystyle Y\in D}"></span> exists such that <span class="mwe-math-element mwe-math-element-inline"><span class="mwe-math-mathml-inline mwe-math-mathml-a11y" style="display: none;"><math xmlns="http://www.w3.org/1998/Math/MathML" alttext="{\displaystyle g\in Y\subseteq B\cup G}">
  <semantics>
    <mrow class="MJX-TeXAtom-ORD">
