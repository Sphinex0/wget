        <mo>=</mo>
        <munder>
          <mo>∑<!-- ∑ --></mo>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>S</mi>
            <mo>⊆<!-- ⊆ --></mo>
            <mi>E</mi>
          </mrow>
        </munder>
        <mo stretchy="false">(</mo>
        <mi>x</mi>
        <mo>−<!-- − --></mo>
        <mn>1</mn>
