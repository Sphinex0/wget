                  <mi>k</mi>
                </mrow>
              </msub>
              <mo stretchy="false">)</mo>
            </mrow>
            <mrow>
              <msup>
                <mi>f</mi>
                <mo>″</mo>
              </msup>
              <mo stretchy="false">(</mo>
