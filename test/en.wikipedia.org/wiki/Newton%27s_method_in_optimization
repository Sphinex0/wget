                <mi>t</mi>
              </mrow>
            </mfrac>
          </mrow>
          <mrow>
            <mo>(</mo>
            <mrow>
              <mi>f</mi>
              <mo stretchy="false">(</mo>
              <msub>
                <mi>x</mi>
                <mrow class="MJX-TeXAtom-ORD">
                  <mi>k</mi>
                </mrow>
              </msub>
              <mo stretchy="false">)</mo>
              <mo>+</mo>
