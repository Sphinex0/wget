              <mo>|</mo>
              <mrow>
                <msub>
                  <mi>y</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>k</mi>
                  </mrow>
                </msub>
                <mo>&#x2212;<!-- − --></mo>
                <mi>S</mi>
              </mrow>
              <mo>|</mo>
            </mrow>
