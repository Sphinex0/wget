                    <mrow class="MJX-TeXAtom-ORD">
                      <mi mathvariant="normal">T</mi>
                    </mrow>
                  </mrow>
                </msup>
                <mrow class="MJX-TeXAtom-ORD">
                  <mi mathvariant="bold-italic">λ<!-- λ --></mi>
                </mrow>
                <mo>+</mo>
