                </mrow>
              </mtd>
              <mtd>
                <mi></mi>
                <mo>=</mo>
                <msup>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mrow>
                      <mo>[</mo>
                      <mtable rowspacing="4pt" columnspacing="1em">
                        <mtr>
                          <mtd>
                            <mn>0</mn>
                          </mtd>
                          <mtd>
                            <mn>0</mn>
                          </mtd>
                        </mtr>
                      </mtable>
