                    <mrow class="MJX-TeXAtom-ORD">
                      <mo>−<!-- − --></mo>
                      <mn>1</mn>
                    </mrow>
                  </msup>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi mathvariant="bold-italic">b</mi>
                  </mrow>
                </mtd>
              </mtr>
