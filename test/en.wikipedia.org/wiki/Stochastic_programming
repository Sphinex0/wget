                      <mover>
                        <mi>g</mi>
                        <mo stretchy="false">^<!-- ^ --></mo>
                      </mover>
                    </mrow>
                  </mrow>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>N</mi>
                  </mrow>
                </msub>
                <mo stretchy="false">(</mo>
                <mi>x</mi>
                <mo stretchy="false">)</mo>
                <mo>=</mo>
              </mtd>
              <mtd>
                <munder>
                  <mo form="prefix">min</mo>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mi>x</mi>
                    <mo>∈<!-- ∈ --></mo>
                    <msup>
                      <mrow class="MJX-TeXAtom-ORD">
