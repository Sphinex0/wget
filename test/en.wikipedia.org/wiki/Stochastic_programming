          </mrow>
        </munderover>
        <mi>Q</mi>
        <mo stretchy="false">(</mo>
        <mi>x</mi>
        <mo>,</mo>
        <msup>
          <mi>ξ<!-- ξ --></mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>j</mi>
          </mrow>
        </msup>
