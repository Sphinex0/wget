                  <mrow class="MJX-TeXAtom-ORD">
                    <mtext>&nbsp;for some&nbsp;</mtext>
                  </mrow>
                  <mi>j</mi>
                  <mrow class="MJX-TeXAtom-ORD">
                    <mtext>&nbsp;such that&nbsp;</mtext>
                  </mrow>
                  <msub>
                    <mi>f</mi>
                    <mrow class="MJX-TeXAtom-ORD">
                      <mi>j</mi>
                    </mrow>
                  </msub>
                  <mo stretchy="false">(</mo>
                  <mi>x</mi>
                  <mo stretchy="false">)</mo>
                  <mo>&gt;</mo>
                  <mn>0</mn>
