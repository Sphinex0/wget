<span class="linenos" data-line="14"></span><span class="w">        </span><span class="k">end</span>
<span class="linenos" data-line="15"></span><span class="w">    </span><span class="k">end</span>
<span class="linenos" data-line="16"></span><span class="w">    </span><span class="k">if</span><span class="w"> </span><span class="p">(</span><span class="n">bestCandidateFitness</span><span class="w"> </span><span class="k">is</span><span class="w"> </span><span class="o">-</span><span class="err">∞</span><span class="p">)</span>
