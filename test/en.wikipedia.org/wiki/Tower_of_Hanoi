          <mrow class="MJX-TeXAtom-ORD">
            <mi>h</mi>
          </mrow>
        </msub>
        <mo>=</mo>
        <mn>2</mn>
        <msub>
          <mi>T</mi>
          <mrow class="MJX-TeXAtom-ORD">
            <mi>h</mi>
            <mo>−<!-- − --></mo>
